#include <stdio.h>
/* a code to grade a subject */
int main (void)
{
float mark;

printf("Enter your mark");
scanf("%f", &mark);
if (mark >= 70)
printf("A");
else if (mark >= 60)
printf("B");
else if (mark >= 50)
printf("C");
else if (mark >= 40)
printf("D");
else if (mark >= 30)
printf("E");
else if (mark >= 0)
printf("F");
else
    printf("no score found");
}
